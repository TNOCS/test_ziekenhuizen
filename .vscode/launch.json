  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Merge data",
      "cwd": "${workspaceFolder}/packages/processing",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/packages/processing/src/index.ts"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Process postcode data",
      "cwd": "${workspaceFolder}/packages/processing",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/packages/processing/src/processPostcodes.ts"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Process ziekenhuis data",
      "cwd": "${workspaceFolder}/packages/processing",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "args": [
        "${workspaceFolder}/packages/processing/src/processZiekenhuizen.ts"
      ]
    }
  ]
}